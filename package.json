  "description": "To automatically apply context-sensitive XSS output filtering for Handlebars",
  "preferGlobal": "true",
  "bin": {
    "handlebarspp": "bin/handlebarspp"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yahoo/secure-handlebars.git"
  },
  "keywords": [
    "xss",
    "security",
    "escape",
    "encode",
    "filter",
    "context",
    "html5",
    "handlebars",
    "parser",
    "precompiler"
