  "scripts": {
    "hint": "grunt jshint",
    "build": "npm install; grunt",
    "clean": "grunt clean",
    "test": "grunt test"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "context-parser": "^2.0.1",
    "glob": "^7.0.6",
    "handlebars": "^3.0.3",
