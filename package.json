    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.0.2",
    "phantomjs": "^2.1.7"
  },
  "main": "./src/secure-handlebars.js",
  "bugs": {
    "url": "https://github.com/yahoo/secure-handlebars/issues"
